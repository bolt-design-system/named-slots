{
  "name": "skatejs-named-slots",
  "build:global": "skatejsNamedSlots",
  "description": "A polygap (partial polyfill) for the Shadow DOM Named Slot API.",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "sk-tests",
    "test/watch": "sk-tests-watch",
    "test/perf": "karma start --single-run --perf",
    "lint": "eslint -c node_modules/skatejs-build/.eslintrc src",
    "prepublish": "sk-bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skatejs/named-slots.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "named",
    "slots",
    "shadow",
    "dom",
    "skate"
  ],
  "author": "Trey Shugart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skatejs/named-slots/issues"
  },
  "homepage": "https://github.com/skatejs/named-slots#readme",
  "devDependencies": {
    "eslint": "^2.11.0",
    "eslint-plugin-react": "^3.16.1",
    "skatejs-build": "^7.0.2",
    "birdpoo": "0.0.2"
  },
  "dependencies": {
    "custom-event-polyfill": "joscha/custom-event-polyfill#de24bd9",
    "debounce": "^1.0.0",
    "webcomponents.js": "webcomponents/webcomponentsjs#b77ca74"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "version": "0.2.0"
}
