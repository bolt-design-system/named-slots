{
  "name": "skatejs-named-slots",
  "build:global": "skatejsNamedSlots",
  "description": "A polygap (partial polyfill) for the Shadow DOM Named Slot API.",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "sk-tests",
    "test/watch": "sk-tests-watch",
    "test/perf": "karma start --single-run --perf",
    "lint": "eslint . --ext .js,.jsx --format 'node_modules/eslint-friendly-formatter'",
    "prepublish": "sk-bundle",
    "commit": "npm run lint && git cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skatejs/named-slots.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "named",
    "slots",
    "shadow",
    "dom",
    "skate"
  ],
  "author": "Trey Shugart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skatejs/named-slots/issues"
  },
  "homepage": "https://github.com/skatejs/named-slots#readme",
  "devDependencies": {
    "birdpoo": "0.0.2",
    "eslint": "^3.2.2",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-plugin-react": "^6.0.0",
    "precommit-hook": "^3.0.0",
    "skatejs-build": "^7.1.0"
  },
  "dependencies": {
    "custom-event-polyfill": "^0.3.0",
    "debounce": "^1.0.0",
    "webcomponents.js": "webcomponents/webcomponentsjs#b77ca74"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "pre-commit": [
    "lint"
  ],
  "version": "0.2.0"
}
