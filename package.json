{
  "name": "skatejs-named-slots",
  "build:global": "skatejsNamedSlots",
  "description": "A polygap (partial polyfill) for the Shadow DOM Named Slot API.",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "sk-tests",
    "test/watch": "sk-tests-watch",
    "test/perf": "sk-tests -- --perf",
    "lint": "sk-lint",
    "prepublish": "sk-bundle",
    "commit": "sk-commit",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skatejs/named-slots.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "named",
    "slots",
    "shadow",
    "dom",
    "skate"
  ],
  "author": "Trey Shugart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skatejs/named-slots/issues"
  },
  "homepage": "https://github.com/skatejs/named-slots#readme",
  "devDependencies": {
    "birdpoo": "0.0.2",
    "eslint": "^3.2.2",
    "eslint-plugin-react": "^6.0.0",
    "skatejs-build": "9.x"
  },
  "dependencies": {
    "custom-event-polyfill": "^0.3.0",
    "debounce": "^1.0.0",
    "weakmap": "0.0.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "version": "0.2.0"
}
